<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="Huatongmis.Data.Entities" assembly="hmbl">
    <class name="Returnticket" table="vw_returnticket" >
        <composite-id>
            <key-property name="OrderNum" column="OrderNum" type="String" length="300" /> 
            <key-property name="Name" column="Name" type="String" length="60" /> 
            <key-property name="TicketCount" column="TicketCount" type="Int32" /> 
            <key-property name="Count" column="ReturnTicketCount" type="Int64" /> 
            <key-property name="Account" column="ReturnTicketAccount" type="Double" /> 
            <key-property name="CreatedOn" column="CreatedOn" type="DateTime" /> 
        </composite-id>
    </class>
</hibernate-mapping>

